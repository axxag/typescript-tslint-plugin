{
  "name": "typescript-eslint-plugin",
  "version": "0.5.5",
  "description": "TypeScript eslint language service plugin",
  "main": "out/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/axxag/typescript-eslint-plugin.git"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "compile": "tsc",
    "test": "mocha ./out/**/*.test.js --slow 2000 --timeout 10000",
    "lint": "eslint --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./src",
    "e2e": "mocha ./e2e/tests/**/*.test.js --slow 2000 --timeout 10000"
  },
  "dependencies": {
    "minimatch": "^3.0.4",
    "mock-require": "^3.0.3",
    "vscode-languageserver": "^5.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/eslint": "^6.8.0",
    "@types/minimatch": "^3.0.3",
    "@types/mocha": "^5.2.5",
    "@types/mock-require": "^2.0.0",
    "@types/node": "^12.12.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^25.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "mocha": "^6.2.2",
    "prettier": "^2.0.5",
    "typescript": "^3.7.2"
  }
}
