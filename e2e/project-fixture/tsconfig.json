{
  "compilerOptions": {
    "plugins": [
      {
        "name": "typescript-eslint-plugin",
        "alwaysShowRuleFailuresAsWarnings": false,
        "ignoreDefinitionFiles": true
        //"configFile": "../eslint.json",
      }
    ],
    "module": "commonjs",
    "target": "es6",
    "allowJs": true,
    "noImplicitAny": false,
    "sourceMap": false,
    "noEmit": true
  }
}
